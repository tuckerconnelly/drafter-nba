{
  "scripts": {
    "dev": "env $([ -f .env ] && cat .env | grep -v ^# | xargs) nodemon --max-old-space-size=8192",
    "loadTeams": "env $([ -f .env ] && cat .env | grep -v ^# | xargs) node --max-old-space-size=8192 src/loadTeams.js; tput bel",
    "loadGames": "env $([ -f .env ] && cat .env | grep -v ^# | xargs) node --max-old-space-size=8192 src/loadGames.js; tput bel",
    "train": "env $([ -f .env ] && cat .env | grep -v ^# | xargs) node --max-old-space-size=22528 src/train.js; tput bel",
    "test": "env $([ -f .env.test ] && cat .env.test | grep -v ^# | xargs) jest --detectOpenHandles --no-cache",
    "prettier": "prettier --write 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "axios-retry": "^3.1.1",
    "bluebird": "^3.5.3",
    "cheerio": "^1.0.0-rc.2",
    "chrono-node": "^1.3.5",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "papaparse": "^4.6.1",
    "pg": "^7.7.1",
    "pg-native": "^3.0.0",
    "recursive-readdir": "^2.2.2",
    "shuffle-seed": "^1.1.6",
    "sqorn-pg": "0.0.40"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "winston": "^3.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-console": 0,
      "semi": 1
    },
    "globals": {
      "logger": false
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "npm run prettier -- ",
        "npm run lint -- ",
        "git add"
      ]
    }
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "tmp",
      "json"
    ],
    "forceExit": true,
    "setupTestFrameworkScriptFile": "./testUtils.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "post-merge": "npm i"
    }
  }
}
